{
    "name": "Hyperion.NG",
    "version": "2.0.0-alpha.7",
    "slug": "hyperion.ng",
    "description": "Hyperion is an opensource Bias or Ambient Lighting implementation which you might know from TV manufacturers.",
    "url": "https://hyperion-project.org/",
    "arch": ["amd64", "armhf"],
    "startup": "application",
    "boot": "auto",
    "ports": {
      "8090/tcp": 8090,
      "19333/tcp": 19333,
      "19400/tcp": 19400,
      "19444/tcp": 19444,
      "19445/tcp": 19445
    },
    "ports_description": {
      "8090/tcp": "Web Admin",
      "19333/tcp": "Boblight Server",
      "19400/tcp": "Google Flatbuffers Receiver",
      "19444/tcp": "JSON-RPC Remote Control",
      "19445/tcp": "Protocol Buffers Server"
    },
    "map": ["config:rw"],
    "image": "bradsjm/hyperion-ng-addon-{arch}",
    "options": {},
    "schema": {},
    "auto_uart": true,
    "usb": true,
    "webui": "http://[HOST]:[PORT:8090]",
    "ingress": true
  }